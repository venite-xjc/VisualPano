{
  "name": "visualpano",
  "displayName": "VisualPano",
  "description": "",
  "publisher": "venite",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.69.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:VisualPano.start"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": 
    [
      {
        "command": "VisualPano.start",
        "title": "VisualPano: Start a panorama view | 开启全景"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "VisualPano.start",
          "group": "z_commands",
          "when": "resourceExtname == .jpg || resourceExtname == .png"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.69.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4",
    "@vscode/test-electron": "^2.1.5"
  }
}
